    var FieldID = '[% Data.ItemID | html %]';
    FieldID = FieldID.replace(/:/g, '\\:');
    Core.Agent.UserSearch.Init($("#"+FieldID), [% Data.ActiveAutoComplete | html %]);
# EO ITSM-CIAttributeCollection-capeIT
//]]></script>
