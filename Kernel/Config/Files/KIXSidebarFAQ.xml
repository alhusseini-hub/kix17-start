                <Item Key="SearchStateTypes">internal</Item>
                <Item Key="SearchSubject">1</Item>
                <Item Key="TicketIDSearchMaskRegExp">AgentTicket(Zoom|PhoneInbound|PhoneOutbound|Note|Compose|ZoomTabActionCommon)</Item>
