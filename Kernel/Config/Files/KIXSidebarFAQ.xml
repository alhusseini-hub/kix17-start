    <ConfigItem Name="Frontend::KIXSidebarBackend###KIXSBSimiliarFAQ" Required="0" Valid="0">
        <Description Translatable="1">Sidebar module registration for the agent interface.</Description>
        <Group>KIXSidebarTools</Group>
        <SubGroup>Frontend::Agent::KIXSidebarFAQ</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Actions">AgentTicket(Zoom|Phone|PhoneInbound|PhoneOutbound|Note|Compose|ZoomTabActionCommon)</Item>
