                <Item Key="Description">Shows similar tickets and linked CIs to current ticket.</Item>
                <Item Key="Prio">0610</Item>
                <Item Key="InitialCollapsed"></Item>
                <Item Key="Identifier">KIXSBSimilarTicketLinkedCIs</Item>
                <Item Key="Group"></Item>
                <Item Key="Callback"></Item>
                <Item Key="LinkType"></Item>
