        <Group>KIXSidebarTools</Group>
        <SubGroup>Frontend::Agent::KIXSidebarTicketLinkCI</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Actions">AgentTicket(Phone|Email|Zoom)</Item>
                <Item Key="Module">Kernel::Output::HTML::KIXSidebar::TicketLinkCI</Item>
